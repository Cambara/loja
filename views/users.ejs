<table border="1" cellpadding="7" cellspacing="7">
   <tr>
       <th width="50px">No</th>
       <th>Nome</th>
       
   </tr>                               
   <% if(data.length){ 
                   
    for(var i = 0;i < data.length;i++) { %>                 
   <tr>
       <td><%=(i+1)%></td>
       <td><%=data[i].nome%></td>
   </tr>
<% }

}else{ %>
    <tr>
       <td colspan="3">No user</td>
    </tr>
<% } %>
                                                
</table>